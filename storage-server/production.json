{
  "apps": [
    {
      "name": "storage-server",
      "script": "app.js",
      "exec_mode": "cluster",
      "instances": 0,
      "merge_logs": true,
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "out_file": "./logs/out.log",
      "error_file": "./logs/error.log",
      "env": {
        "PORT": "10240",
        "NODE_ENV": "production"
      }
    }
  ]
}
